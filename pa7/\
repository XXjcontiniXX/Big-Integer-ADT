#include "Dictionary.h"
#include <iostream>

using namespace std;

int main () {
	 Dictionary A = Dictionary();
	 A.setValue("d", 1);
    A.setValue("b", 5);
    A.setValue("a", 10);
    A.setValue("c", 15);
    A.setValue("f", 20);
    A.setValue("e", 25);
    A.setValue("g", 30);
    //A.begin();
	 cout << "you gotta be jokin\n";
	 //A.remove("g");
    A.remove("a");
	 
	 
    if (A.hasCurrent()) {
      cout << "test 1\n";
		return 0;
	}
    A.end();
    A.remove("d");
    if (A.currentKey() != "g" || A.currentVal() != 30) {
    	cout << "test 2\n";
      return 0;
	 }	 
	 A.remove("g");
    if (A.hasCurrent()) {
      cout << "test 3\n";
      return 0;
	}
  }

